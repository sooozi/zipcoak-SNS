{
    "compilerOptions": {
        "outDir": "./dist",
        "module": "commonjs",
        "target": "es6", // es5에서 es6로 변경
        "noEmit": false,
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "jsx": "react" // JSX 문법 사용을 위해 추가
    },
    "include": ["src/**/*"], // src 폴더 내 모든 파일 포함
    "exclude": ["dist"] // dist 폴더 제외
}
