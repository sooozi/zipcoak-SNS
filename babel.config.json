    "presets": [
        [
            "@babel/preset-env",
            "@babel/preset-react",
            "@babel/preset-typescript",
            {
                "targets": {
                    "chrome": "64",
                    "edge": "79",
                    "firefox": "67",
                    "opera": "51",
                    "safari": "12"
                }
            }
        ],
        "@babel/preset-react",
        "@babel/preset-typescript" // TypeScript 지원 추가
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "corejs": 3 // "useBuiltIns": "usage" 제거
            }
        ]
    ]
